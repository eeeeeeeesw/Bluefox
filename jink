        repeat wait() until game:IsLoaded() == true

if game.PlaceId == 1204965839 then 
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Fattythefatty/bluefoxscript/main/bluefox%20script.lua"))();

else
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Fattythefatty/bluefoxscript/main/NSWbus"))();
